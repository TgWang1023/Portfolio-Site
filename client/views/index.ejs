                    <img class="col-3 skill-image" src="img/socket.png" alt="socket">
                    <img class="col-3 skill-image" src="img/react.png" alt="react">
                </div>
                <div class="row mt-3">
                    <img class="col-4 skill-image" src="img/xcode.png" alt="xcode">
                    <img class="col-4 skill-image" src="img/swift.png" alt="swift">
                    <img class="col-4 skill-image" src="img/spritekit.png" alt="spritekit">
                </div>
                <div class="row mt-3">
                    <img class="col-3 skill-image" src="img/typescript.png" alt="typescript">
                    <img class="col-3 skill-image" src="img/c++.png" alt="c++">
                    <img class="col-3 skill-image" src="img/aws.png" alt="aws">
                    <img class="col-3 skill-image" src="img/postman.png" alt="postman">
                </div>
            </div> 
